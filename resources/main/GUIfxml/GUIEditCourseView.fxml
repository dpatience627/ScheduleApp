<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text fx:id="courseText" layoutX="250.0" layoutY="60.0" strokeType="OUTSIDE" strokeWidth="0.0" text="All sorts of course info will go here" textAlignment="CENTER" wrappingWidth="500.0">
         <font>
            <Font size="18.0" />
         </font></Text>
      <Separator layoutX="497.0" layoutY="224.0" orientation="VERTICAL" prefHeight="300.0" />
      <Text layoutX="59.0" layoutY="250.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Some courses are offered in several timeslots at once, as denoted by having an A, B, etc. in the course code. If you would like to switch the timeslot for this course, click the button below to search, then select a timeslot." wrappingWidth="408.00001525878906" />
      <ListView fx:id="searchResultsView" layoutX="57.0" layoutY="328.0" prefHeight="114.0" prefWidth="400.0" />
      <Button layoutX="59.0" layoutY="300.0" mnemonicParsing="false" onAction="#doSearch" text="Search" />
      <Button fx:id="addButton" disable="true" layoutX="59.0" layoutY="500.0" mnemonicParsing="false" onAction="#switchCourse" prefHeight="51.0" prefWidth="400.0" text="~ Select a Course ~">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button layoutX="648.0" layoutY="361.0" mnemonicParsing="false" onAction="#removeCourse" text="Remove this course from semester" textFill="RED">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Text fx:id="courseSearchInfoText" layoutX="57.0" layoutY="463.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Cash and lots of it" wrappingWidth="400.0" />
      <Button layoutX="30.0" layoutY="25.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
   </children>
</AnchorPane>
