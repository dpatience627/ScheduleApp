<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
   <class name = "edu.gcc.comp350.amazeall.LogEntry" table = "restore_table">

        <id name = "schedName" type = "string" access="field" column = "Sched_Name" >
            <generator class="native"/>
        </id>
        <property name = "action" column = "Log_Action" access="field" type = "string"/>
       <property name= "semesterNumber"  column="Semester_Num" access="field" type="int"/>
       <property name = "courseID" column = "Course_ID" access="field" type = "string"/>

   </class>
</hibernate-mapping>